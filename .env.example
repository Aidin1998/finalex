# Database
PINCEX_DATABASE_DSN=postgres://postgres:postgres@localhost:5432/pincex?sslmode=disable

# Redis
REDIS_ADDRESS=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Server
SERVER_PORT=8080

# JWT
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRATION_HOURS=24
JWT_REFRESH_SECRET=your-refresh-secret-key-change-this-in-production
JWT_REFRESH_EXPIRATION_HOURS=168

# KYC
KYC_PROVIDER_URL=https://kyc-provider.example.com
KYC_PROVIDER_API_KEY=your-kyc-provider-api-key
KYC_DOCUMENT_BASE_PATH=/var/lib/pincex/kyc-documents

# Logging
LOG_LEVEL=info
