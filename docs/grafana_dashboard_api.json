{
  "dashboard": {
    "title": "API Endpoint Health & Error Rates",
    "panels": [
      {
        "type": "graph",
        "title": "API Request Rate",
        "targets": [{"expr": "sum(rate(pincex_http_requests_total[1m])) by (path)"}],
        "datasource": "Prometheus"
      },
      {
        "type": "graph",
        "title": "API Request Latency (P95)",
        "targets": [{"expr": "histogram_quantile(0.95, sum(rate(pincex_http_request_duration_seconds_bucket[5m])) by (le, path))"}],
        "datasource": "Prometheus"
      },
      {
        "type": "graph",
        "title": "API 5xx Error Rate",
        "targets": [{"expr": "sum(rate(api_5xx_responses_total[5m])) by (path)"}],
        "datasource": "Prometheus"
      }
    ]
  }
}
