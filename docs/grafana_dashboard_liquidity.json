{
  "title": "Liquidity & Market Making",
  "panels": [
    {
      "type": "graph",
      "title": "Order Book Depth (Top 5 Levels)",
      "targets": [
        { "expr": "pincex_orderbook_depth{level=\"1\"}", "legendFormat": "Level 1" },
        { "expr": "pincex_orderbook_depth{level=\"2\"}", "legendFormat": "Level 2" },
        { "expr": "pincex_orderbook_depth{level=\"3\"}", "legendFormat": "Level 3" },
        { "expr": "pincex_orderbook_depth{level=\"4\"}", "legendFormat": "Level 4" },
        { "expr": "pincex_orderbook_depth{level=\"5\"}", "legendFormat": "Level 5" }
      ]
    },
    {
      "type": "stat",
      "title": "Spread (bps)",
      "targets": [
        { "expr": "pincex_orderbook_spread", "legendFormat": "Spread" }
      ]
    },
    {
      "type": "stat",
      "title": "Market Maker Inventory",
      "targets": [
        { "expr": "pincex_mm_inventory", "legendFormat": "Inventory" }
      ]
    },
    {
      "type": "table",
      "title": "Liquidity Provider Performance",
      "targets": [
        { "expr": "sum by (provider) (pincex_lp_volume)", "legendFormat": "{{provider}}" }
      ]
    }
  ]
}
